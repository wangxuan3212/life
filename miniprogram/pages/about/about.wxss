/* pages/about/about.wxss */
page{
 background: #f4f4f4;
}
.top{
  text-align:center;
  font-size:18px;
  width:100%;
  height:50px;
  line-height:50px;
  background:#fff;
}
.fixed{
  position:fixed;
  z-index: 999;
  top:0;
  left:0
}
.top_image{
  width:100%;
  height:180px;
  margin-top:53px;
  position: relative;
}
.top_image image{
  width:100%;
  height: 100%;
}
.inner{
  width:80%;
  height:250px;
  background:#fff;
  margin-left:10%;
  border-radius: 8px;
  margin-top:105px;
  
}
.inner_top{
  width:100%;
  height:125px;
  padding:10px ;
  box-sizing: border-box;
}
.company{
  font-size:16px;
  padding-left:10px;
  color:#666;
  border-left:3px solid #666;
}
.address{
  font-size:14px;
  color:#333;
  margin-top:10px;
}
.car{
  width:80%;
  height:120px;
  background: #fff;
  margin-left: 10%;
  border-radius: 8px;
  position: absolute;
  bottom: -80px;
  left:0;

}
.login{
  width:80px;
  height:80px;
  position: absolute;
  bottom:40%;
  left:50%;
  margin-left:-40px;
  text-align:center;
}
.circle{
  width:60px;
  height:60px;
  background: #fff;
  margin-left:10px;
  margin-bottom: 5px;
  border-radius: 50%;
}
.goods{
  width:100%;
  height:30px;
  text-align:center;
  font-size:16px;
  color:#333;
  line-height: 30px;
  margin-top:10px;
}
.my_car{
  width:100%;
  height:130rpx;
  display:flex;
  justify-content: space-between;
  align-items: flex-start;
}
.car_item{
  flex:1;
  height:100%;
}
.car_item image{
  width:35%;
  margin-left:33%;
  height:35%;
  margin-top:15%;
}
.state{
  font-size:22rpx;
  color:#333;
  margin-top:20rpx;
  text-align:center;
}
